@model IEnumerable<SolutionNorSolutionPim.BusinessLogicLayer.ProductHistoryContract>

@{
    ViewBag.Title = "Product History";
}

<h2>Product History</h2>

<table class="table">
    <tr>
        <th>Product Name</th>
        <th>State Name</th>
        <th>Date Time</th>
        <th>User Name</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductName) </td>
            <td>@Html.DisplayFor(modelItem => item.StateName) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.DisplayFor(modelItem => item.UserName) </td>
        </tr>
    }

</table>
