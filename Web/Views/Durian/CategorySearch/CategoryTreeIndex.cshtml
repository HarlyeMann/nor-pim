@model IEnumerable<SolutionNorSolutionPim.BusinessLogicLayer.CategoryTreeContract>

@{
    ViewBag.Title = "Category Tree";
}

<h2>Category Tree</h2>

<table class="table">
    <tr>
        <th>Product Category Code</th>
        <th>Product Category Name</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductCategoryCode) </td>
            <td>@Html.DisplayFor(modelItem => item.ProductCategoryName) </td>
        </tr>
    }

</table>
