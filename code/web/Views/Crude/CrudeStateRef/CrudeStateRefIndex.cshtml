@model IEnumerable<norpim.BusinessLogicLayer.CrudeStateRefContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeStateRefCreate")
</p>

<table class="table">
    <tr>
        <th>State</th>
        <th>State Name</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.StateRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.StateName) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeStateRefDetails", new { stateRcd = item.StateRcd }) |
                @Html.ActionLink("Edit", "CrudeStateRefEdit", new { stateRcd = item.StateRcd }) |
                @Html.ActionLink("New", "CrudeStateRefCreate", new { })
            </td>
        </tr>
    }

</table>
