@model IEnumerable<norpim.BusinessLogicLayer.CrudeProductGatherKeyContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeProductGatherKeyCreate")
</p>

<table class="table">
    <tr>
        <th>Product Gather Key Value</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductGatherKeyValue) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeProductGatherKeyDetails", new { productGatherKeyId = item.ProductGatherKeyId }) |
                @Html.ActionLink("Edit", "CrudeProductGatherKeyEdit", new { productGatherKeyId = item.ProductGatherKeyId }) |
                @Html.ActionLink("New", "CrudeProductGatherKeyCreate", new { })
            </td>
        </tr>
    }

</table>
