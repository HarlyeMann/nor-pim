@model IEnumerable<norpim.BusinessLogicLayer.CrudeUserContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeUserCreate")
</p>

<table class="table">
    <tr>
        <th>User Code</th>
        <th>User Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>State</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.UserCode) </td>
            <td>@Html.DisplayFor(modelItem => item.UserName) </td>
            <td>@Html.DisplayFor(modelItem => item.Email) </td>
            <td>@Html.DisplayFor(modelItem => item.Password) </td>
            <td>@Html.DisplayFor(modelItem => item.StateRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeUserDetails", new { userId = item.UserId }) |
                @Html.ActionLink("Edit", "CrudeUserEdit", new { userId = item.UserId }) |
                @Html.ActionLink("New", "CrudeUserCreate", new { })
            </td>
        </tr>
    }

</table>
