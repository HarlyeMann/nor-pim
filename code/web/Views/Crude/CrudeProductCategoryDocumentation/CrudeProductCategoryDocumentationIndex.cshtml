@model IEnumerable<norpim.BusinessLogicLayer.CrudeProductCategoryDocumentationContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeProductCategoryDocumentationCreate")
</p>

<table class="table">
    <tr>
        <th>Product Category Documentation Type</th>
        <th>Documentation</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductCategoryDocumentationTypeRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.Documentation) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeProductCategoryDocumentationDetails", new { productCategoryDocumentationId = item.ProductCategoryDocumentationId }) |
                @Html.ActionLink("Edit", "CrudeProductCategoryDocumentationEdit", new { productCategoryDocumentationId = item.ProductCategoryDocumentationId }) |
                @Html.ActionLink("New", "CrudeProductCategoryDocumentationCreate", new { })
            </td>
        </tr>
    }

</table>
