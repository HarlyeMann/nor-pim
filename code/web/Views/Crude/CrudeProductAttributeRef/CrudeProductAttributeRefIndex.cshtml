@model IEnumerable<norpim.BusinessLogicLayer.CrudeProductAttributeRefContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeProductAttributeRefCreate")
</p>

<table class="table">
    <tr>
        <th>Product Attribute</th>
        <th>Product Attribute Name</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductAttributeRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.ProductAttributeName) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeProductAttributeRefDetails", new { productAttributeRcd = item.ProductAttributeRcd }) |
                @Html.ActionLink("Edit", "CrudeProductAttributeRefEdit", new { productAttributeRcd = item.ProductAttributeRcd }) |
                @Html.ActionLink("New", "CrudeProductAttributeRefCreate", new { })
            </td>
        </tr>
    }

</table>
