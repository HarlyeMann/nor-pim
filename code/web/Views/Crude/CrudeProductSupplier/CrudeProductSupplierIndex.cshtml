@model IEnumerable<norpim.BusinessLogicLayer.CrudeProductSupplierContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeProductSupplierCreate")
</p>

<table class="table">
    <tr>
        <th>Supplier Name</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.SupplierName) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeProductSupplierDetails", new { productSupplierId = item.ProductSupplierId }) |
                @Html.ActionLink("Edit", "CrudeProductSupplierEdit", new { productSupplierId = item.ProductSupplierId }) |
                @Html.ActionLink("New", "CrudeProductSupplierCreate", new { })
            </td>
        </tr>
    }

</table>
