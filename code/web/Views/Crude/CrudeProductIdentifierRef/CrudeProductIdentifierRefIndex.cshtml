@model IEnumerable<norpim.BusinessLogicLayer.CrudeProductIdentifierRefContract>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CrudeProductIdentifierRefCreate")
</p>

<table class="table">
    <tr>
        <th>Product Identifier</th>
        <th>Product Identifier Name</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.ProductIdentifierRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.ProductIdentifierName) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>@Html.ActionLink("Details", "CrudeProductIdentifierRefDetails", new { productIdentifierRcd = item.ProductIdentifierRcd }) |
                @Html.ActionLink("Edit", "CrudeProductIdentifierRefEdit", new { productIdentifierRcd = item.ProductIdentifierRcd }) |
                @Html.ActionLink("New", "CrudeProductIdentifierRefCreate", new { })
            </td>
        </tr>
    }

</table>
